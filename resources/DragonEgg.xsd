<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
			targetNamespace="http://www.epam.com/trapeznikau/dragon_egg"
			xmlns:tns="http://www.epam.com/trapeznikau/dragon_egg"
			xmlns:trev="http://www.epam.com/trapeznikau/abstract_treasure_valueable"
			elementFormDefault="qualified">
			
			<xsd:import schemaLocation="AbstractTreasureValueable.xsd" namespace="http://www.epam.com/trapeznikau/abstract_treasure_valueable"/>
	
	<xsd:complexType name="DragonEgg">
		<xsd:complexContent>
			<xsd:extension base="trev:AbstractTreasureValueable">
				<xsd:sequence>
					<xsd:element name="size" type="xsd:string"/>
					<xsd:element name="type_dragon" type="xsd:string"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
</xsd:schema>